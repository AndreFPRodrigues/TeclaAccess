<?xml version="1.0" encoding="utf-8"?>
<TableLayout xmlns:android="http://schemas.android.com/apk/res/android"
    style="@style/cheatsheettable" >

    <TableRow
        android:id="@+id/tableRow1"
        style="@style/cheatsheettoprow" >

        <TextView
            style="@style/symbol"
            android:text="B" />

        <TextView
            style="@style/code"
            android:text="-•••" />
        
        <TextView
            style="@style/symbol"
            android:text="O" />

        <TextView
            style="@style/code"
            android:text="---" />

        <TextView
            style="@style/symbol"
            android:text="6" />

        <TextView
            style="@style/code"
            android:text="-••••" />
                 
        <TextView
            style="@style/symbol"
            android:text="," />

        <TextView
            style="@style/code"
            android:text="--••--" />
        
        <TextView
            style="@style/symbol"
            android:text="&amp;" />

        <TextView
            style="@style/code_nopadding"
            android:text="-••--" />
    </TableRow>

    <TableRow style="@style/cheatsheetrow" >

        <TextView
            style="@style/symbol"
            android:text="C" />

        <TextView
            style="@style/code"
            android:text="-•-•" />
       
        <TextView
            style="@style/symbol"
            android:text="Q" />

        <TextView
            style="@style/code"
            android:text="--•-" />
                
        <TextView
            style="@style/symbol"
            android:text="7" />

        <TextView
            style="@style/code"
            android:text="--•••" />     
                       
        <TextView
            style="@style/symbol"
            android:text=":" />

        <TextView
            style="@style/code"
            android:text="-•-•-•" />
                
        <TextView
            style="@style/symbol"
            android:text="$" />

        <TextView
            style="@style/code_nopadding"
            android:text="-•••-•" />
    </TableRow>


    <TableRow style="@style/cheatsheetrow" >
        
        <TextView
            style="@style/symbol"
            android:text="D" />

        <TextView
            style="@style/code"
            android:text="-••" />

        <TextView
            style="@style/symbol"
            android:text="T" />

        <TextView
            style="@style/code"
            android:text="-" />
        
        <TextView
            style="@style/symbol"
            android:text="8" />

        <TextView
            style="@style/code"
            android:text="---••" />
                
        <TextView
            style="@style/symbol"
            android:text="&quot;" />

        <TextView
            style="@style/code"
            android:text="--•--" />
                
        <TextView
            style="@style/symbol"
            android:text="#" />

        <TextView
            style="@style/code_nopadding"
            android:text="-•---" />
    </TableRow>

    <TableRow style="@style/cheatsheetrow" >

        <TextView
            style="@style/symbol"
            android:text="G" />

        <TextView
            style="@style/code"
            android:text="--•" />

        <TextView
            style="@style/symbol"
            android:text="X" />

        <TextView
            style="@style/code"
            android:text="-••-" />
                      
        <TextView
            style="@style/symbol"
            android:text="9" />

        <TextView
            style="@style/code"
            android:text="----•" />  
        
        <TextView
            style="@style/symbol"
            android:text="/" />

        <TextView
            style="@style/code"
            android:text="--••-" />
                
        <TextView
            style="@style/symbol"
            android:text=")" />

        <TextView
            style="@style/code_nopadding"
            android:text="-••--•" />
    </TableRow>

    <TableRow style="@style/cheatsheetrow" >

        <TextView
            style="@style/symbol"
            android:text="K" />

        <TextView
            style="@style/code"
            android:text="-•-" />

        <TextView
            style="@style/symbol"
            android:text="Y" />

        <TextView
            style="@style/code"
            android:text="-•--" />
        
        <TextView
            style="@style/symbol"
            android:text="0" />

        <TextView
            style="@style/code"
            android:text="-----" />
                
        <TextView
            style="@style/symbol"
            android:text="\\" />

        <TextView
            style="@style/code"
            android:text="-•••••" />
                
        <TextView
            style="@style/symbol"
            android:text="✓" />

        <TextView
            style="@style/code_nopadding"
            android:text="-•-••" />
    </TableRow>

    <TableRow style="@style/cheatsheetrow" >

        <TextView
            style="@style/symbol"
            android:text="M" />

        <TextView
            style="@style/code"
            android:text="--" />

        <TextView
            style="@style/symbol"
            android:text="Z" />

        <TextView
            style="@style/code"
            android:text="--••" />
        
        <TextView
            style="@style/symbol"
            android:text="-" />

        <TextView
            style="@style/code"
            android:text="-••••-" />
                
        <TextView
            style="@style/symbol"
            android:text="&lt;" />

        <TextView
            style="@style/code"
            android:text="-•--•-" />
                
        <TextView
            style="@style/symbol"
            android:background="@drawable/sym_keyboard_delete_3" />

        <TextView
            style="@style/code_nopadding"
            android:text="----" />
    </TableRow>


    <TableRow style="@style/cheatsheetbottomrow" >

        <TextView
            style="@style/symbol"
            android:text="N" />

        <TextView
            style="@style/code"
            android:text="-•" />

        <TextView
            style="@style/symbol"
            android:text="" />

        <TextView
            style="@style/code"
            android:text="" />
                
        <TextView
            style="@style/symbol"
            android:text="_" />

        <TextView
            style="@style/code"
            android:text="---•" />
        
        <TextView
            style="@style/symbol"
            android:text=">" />

        <TextView
            style="@style/code"
            android:text="--••-•" />
                
        <TextView
            style="@style/symbol"
            android:text="↶" />

        <TextView
            style="@style/code_nopadding"
            android:text="-•••-" />
    </TableRow>

</TableLayout>
